{
  "id": null,
  "title": "Books Async Scraper",
  "tags": [
    "books",
    "scraper",
    "prometheus"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "1s",
  "panels": [
    {
      "type": "stat",
      "title": "Scrape Duration (s)",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "scrape_duration_seconds{job=\"books_async\"}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "type": "stat",
      "title": "Categories Count",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "categories_count{job=\"books_async\"}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "type": "stat",
      "title": "Books Found",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "books_found_total{job=\"books_async\"}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "type": "stat",
      "title": "Books Parsed",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "books_parsed_total{job=\"books_async\"}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "type": "stat",
      "title": "Books Errors",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "books_errors_total{job=\"books_async\"}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "type": "stat",
      "title": "HTTP Requests",
      "gridPos": {
        "x": 6,
        "y": 4,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "http_requests_total{job=\"books_async\"}",
          "refId": "A",
          "instant": true
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Request Duration (p95)",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"books_async\"}[1m])) by (le))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "barchart",
      "title": "Books per Category",
      "gridPos": {
        "x": 0,
        "y": 10,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "expr": "category_books_count{job=\"books_async\"}",
          "refId": "A",
          "instant": true
        }
      ]
    }
  ]
}
